<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.wahaha.uilayouttest">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Linear"
            android:label="MyLinear">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Linear2"
            android:label="MyLinear2" />
        <activity
            android:name=".Linear3"
            android:label="MyLinear3" />
        <activity
            android:name=".Relative"
            android:label="MyRelative" />
        <activity android:name=".Relative2" android:label="MyRelative2"></activity>
    </application>

</manifest>